      "target": "wait-manual",
      "sourceHandle": "false",
      "label": "Can't auto-fix"
    },
    {
      "id": "e4",
      "source": "tester",
      "target": "wait-approve-fix"
    },
    {
      "id": "e5",
      "source": "wait-approve-fix",
      "target": "check-approve-fix"
    },
    {
      "id": "e6-yes",
      "source": "check-approve-fix",
